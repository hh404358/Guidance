  <view class="body">

    <map enable-3D id="map" longitude="{{logitude}}" latitude="{{latitude}}" show-location="true" >
    <maker longitude="119.19245" latitude="26.056212" ></maker>
    </map>

    <view class="block1" bind:tap="goIndoor">AR</view>
    <view class="block2" bind:tap="goBack">
      <image src="{{current?'../../assests/current.svg':'../../assests/goback.svg'}}" class="goback"></image>
    </view>
  </view>
  <view class="mask">

      <upframe style="z-index: 10;"> </upframe>

  
  
 </view>

</view>