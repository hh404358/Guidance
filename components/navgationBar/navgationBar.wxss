/* component/navgationBar/navgationBar.wxss */
.navbody {
  position: relative;
  background-color: white;
}
.nav {
  position: fixed;
  width: 100%;
  padding-left: 46rpx;
  z-index: 3;
  background-color: white;
}
.navBlock {
  position: absolute;
  width: 100%;
}
.navTitle {
  position: fixed;
  width: 138rpx;
  height: 32rpx;
  font-weight: bold;
  
}
.tabBar {
  position: fixed;
  width: 100%;
  height: 104rpx;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.bar {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.barImage {
  width: 58rpx;
  height: 58rpx;

}
.barText {
  width: 20px;
  height: 12px;
  opacity: 1;
  /** 文本1 */
  font-size: 10px;
  font-weight: 500;
  letter-spacing: 0px;
  line-height: 28.96rpx;
  color: rgba(212, 48, 48, 1);
}